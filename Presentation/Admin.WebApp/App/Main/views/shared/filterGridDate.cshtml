@using Admin.WebApp.Resources
@model Admin.WebApp.Models.FilterGridDialogModel

@{
    Layout = "~/App/Main/views/shared/_filterGridLayout.cshtml";
}

<div class="form-group" style="width: 200px">
    <label for="date-value1">@Resource.Label_From</label>
    <input class="form-control" ng-model="@Model.NgModelValue1" id="date-value1" type="datetime-local">
</div>
<div class="form-group" style="width: 200px">
    <label for="date-value2">@Resource.Label_To</label>
    <input class="form-control" ng-model="@Model.NgModelValue2" id="date-value2" type="datetime-local">
</div>